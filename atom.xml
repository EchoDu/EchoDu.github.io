    <title>华为机试HJ6</title>
    <link href="http://example.com/2021/10/21/%E5%8D%8E%E4%B8%BA%E6%9C%BA%E8%AF%95HJ6/"/>
    <id>http://example.com/2021/10/21/%E5%8D%8E%E4%B8%BA%E6%9C%BA%E8%AF%95HJ6/</id>
    <published>2021-10-21T13:25:04.000Z</published>
